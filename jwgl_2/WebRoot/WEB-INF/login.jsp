<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><%	String path = request.getContextPath();			String basePath = request.getScheme() + "://"					+ request.getServerName() + ":" + request.getServerPort()					+ path + "/";%><html xmlns="http://www.w3.org/1999/xhtml" lang="zh"><head><link rel="stylesheet" type="text/css" href="css/style.css"	rel="stylesheet"><title>教务管理系统</title></head><body>	<div class="box">		<div style="float:left">			<image src="images/Library_.png" height=100px;weight=100px;></image>		</div>		<div id="jw">			<image src="images/jwgl.png" height=60px;width=300px;></image>		</div>		<div id="fo">			<form action="login" method="post">				<table>					<tbody>						<tr>							<td><div class="label">用户名:</div></td>							<td><input type="text" name="user.userId" class="inText" id="idnum" ></td>							<td><input type="submit" value="登陆" id="loginBtn"								tabindex="4"></td>						</tr>						<tr>							<td>								<div class="label">密 码:</div>							</td>							<td><input type="password" name="user.password" class="inText" id="pwd">							</td>						</tr>						<tr>							<td><div class="label">类 型:</div></td>							<td><input checked="true" type="radio" name="type"								value="admin">教务员 <input type="radio" name="type"								value="teacher">教师 <input type="radio" name="type"								value="student">学生</td>							<td><input type="reset" value="重置" class="loginReset"></td>						</tr>					</tbody>				</table>			</form>		</div>	</div></body><script type="text/javascript" src="js/jquery.min.js"></script>	<script type="text/javascript" src="js/login.js"></script></html>